, set] = useState(0);\n  const onClick = useCallback(() => set((state) => (state + 1) % 3), []);\n  const transitions = useTransition(index, (p) => p, {\n    from: { opacity: 0, transform: \"translate3d(100%,0,0)\" },\n    enter: { opacity: 1, transform: \"translate3d(0%,0,0)\" },\n    leave: { opacity: 0, transform: \"translate3d(-50%,0,0)\" },\n  });\n  return (\n    <GlobalProvider>\n      <Header />\n\n      <div className='dashboard'>\n        <div className='container-two'>\n          <h1 className='dash-title'>Dashboard</h1>\n\n          <h4 className='subtitle'>WHERE'D MY MONEY GO?</h4>\n          <IncomeExpenses />\n        </div>\n        <div className='container-one' onClick={onClick}>\n          <Link to='/tools/'>Tools</Link>\n          {transitions.map(({ item, props, key }) => {\n            const Page = pages[item];\n            return <Page key={key} style={props} />;\n          })}\n        </div>\n        <div className='container-three'>\n          <TransactionList />\n        </div>\n      </div>\n    </GlobalProvider>\n  );\n};\n",["129","130"],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/components/Balance.js",[],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/pages/Tools.js",[],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/components/Chart.js",["131","132","133","134","135"],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/pages/MakeEntry.js",["136","137","138","139","140","141"],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/context/GlobalState.js",[],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/components/lineChart.js",["142","143","144","145","146"],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/components/Transaction.js",[],"/Users/julie/Documents/Coder Academy/Assignments/ExpenseMe2.0/client/src/pages/Holidays.js",[],{"ruleId":"147","severity":1,"me