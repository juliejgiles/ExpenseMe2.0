 => (\n    <animated.div style={{ ...style, background: \"white\" }}>\n      <LineChart />\n    </animated.div>\n  ),\n];\n\nexport const Dashboard = () => {\n  const modalRef = useRef();\n  const [index, set] = useState(0);\n  const onClick = useCallback(() => set((state) => (state + 1) % 3), []);\n  const transitions = useTransition(index, (p) => p, {\n    from: { opacity: 0, transform: \"translate3d(100%,0,0)\" },\n    enter: { opacity: 1, transform: \"translate3d(0%,0,0)\" },\n    leave: { opacity: 0, transform: \"translate3d(-50%,0,0)\" },\n  });\n\n  return (\n    <GlobalProvider>\n      <Header />\n      <div className='dashboard'>\n        <div className='buttonz'>\n          <div\n            className='button'\n            id='button-3'\n            onClick={() => modalRef.current.open()}\n          >\n            <div id='circle'></div>\n            <a>Make An Entry</a>\n          </div>\n\n          <div className='arrow-down' id='arrow-down'></div>\n\n          <Link to='/tools'>\n            <div class='button' id='button-7'>\n              <div id='dub-arrow'>\n                <img\n                  src='https://www.flaticon.com/svg/vstatic/svg/1077/1077198.svg?token=exp=1612690504~hmac=f4078d1ed3006a9ad20c938b81384c42'\n                  alt=''\n                />\n              </div>\n\n              <a>Use The Tools</a>\n            </div>\n          </Link>\n        </div>\n\n        <div className='container-two'>\n          <h1 className='dash-title'>Dashboard</h1>\n\n          <h4 className='subtitle'>WHERE'D MY MONEY GO?</h4>\n\n          <IncomeExpenses />\n        </div>\n        <div className='container-one' onClick={onClick}>\n          {transitions.map(({ item, props, key }) => {\n            const Page = pages[item];\n            return <Page key={key} style={props} />;\n          })}\n        </div>\n\n        <Modal ref={modalRef}>\n          <div className='modal-move'>\n            <MakeEntry />\n          </div>\n        </Modal>